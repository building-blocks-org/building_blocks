{
    "pythonVersion": "3.9",
    "typeCheckingMode": "basic",
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.*"
    ],
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonPlatform": "All",
    "executionEnvironments": [
        {
            "root": "src",
            "pythonVersion": "3.9"
        }
    ]
}
